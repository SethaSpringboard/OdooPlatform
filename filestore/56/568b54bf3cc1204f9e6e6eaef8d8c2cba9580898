odoo.define('@mass_mailing/../tests/tours/mailing_portal_unsubscribe_from_list', ['@web/core/registry'], function (require) {
'use strict';
let __exports = {};
/** @odoo-module **/

const { registry } = require("@web/core/registry");

/*
